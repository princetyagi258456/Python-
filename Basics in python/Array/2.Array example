from array import *
arr=array('i',[])
n=int(input('Enter the size of the array'))
for i in range(n):
      a=int(input('Enter the next value'))
      arr.append(a)
print(arr)
k=0
val=int(input('Enter the element you for search'))
for j in arr:
      if j==val:
            print(k)
            break
      k+=1

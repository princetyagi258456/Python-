mystring='hello'
mylist=[]

for letter in mystring:
    mylist.append(letter)
mylist

#second method to use append
mylist=[letter for  letter in mystring]
mylist

#another example of this
mylist=[xyz for xyz in 'word']
mylist

#another example of this
mylist=[x**2 for x in range(0,51)]
mylist

#another example of this
mylist=[x for x in range(0,51) if x%2==0]
mylist

#another example of this
results=[x if x%2==0 else 'ODD' for x in range(0,11)]
results


    
